            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".transparent.TransparentActivity"
            android:theme="@style/Theme.AppCompat.Transparent.NoActionBar" />
        <activity android:name=".flexbox.FlexBoxActivity" />
        <activity android:name=".viewpager.ViewPagerActivity" />
        <activity android:name=".swipeRefreshlayout.SwipeRefreshLayoutActivity" />

        <meta-data
